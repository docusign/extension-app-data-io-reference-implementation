{
	"name": "Data Read App",
	"description": {
	  "short": "App for data io",
	  "long": "This app is designed to allow users to use data io extensions"
	},
	"icon": {
	  "data": "",
	  "mediaType": "image/png"
	},
	"screenshots": [],
	"publisher": {
	  "name": "John Doe",
	  "email": "john.doe@gmail.com",
	  "phone": "800-867-5309",
	  "website": "www.johndoe.com"
	},
	"termsOfServiceUrl": "www.johndoe.com/tos",
	"privacyUrl": "www.johndoe.com/privacy",
	"supportUrl": "www.johndoe.com/support",
	"connections": [
	  {
		"name": "authentication",
		"description": "secure connection to the data io proxy",
		"type": "oauth2",
		"params": {
		  "provider": "CUSTOM",
		  "scopes": [],
		  "clientId": "<CLIENT_ID>",
		  "clientSecret": "<CLIENT_SECRET>",
		  "customConfig": {
			"tokenUrl": "<PROXY_BASE_URL>/api/oauth/token",
			"authorizationUrl": "<PROXY_BASE_URL>/api/oauth/authorize",
			"authorizationParams": {
			  "access_type": "offline",
			  "prompt": "consent"
			},
			"authorizationMethod": "header",			
			"scopeSeparator": " ",
			"requiredScopes": []
		  }
		}

	  }
	],
	"extensions": [
	  {
		"name": "My Data IO Extension",
		"description": "using in data io flow",
		"template": "DataIO.Version6.DataInputOutput",
		"actionReferences": [
		  "SearchRecords Action",
		  "GetTypeNames Action",
		  "GetTypeDefinitions Action"
		]
	  }
	],
	"actions": [
	  {
		"name": "SearchRecords Action",
		"description": "something about data io to a proxy",
		"template": "DataIO.Version6.SearchRecords",
		"connectionsReference": "authentication",
		"params": {
		  "uri": "<PROXY_BASE_URL>/api/dataio/searchRecords"
		}
	  },
	  {
		"name": "GetTypeNames Action",
		"description": "something about data io to a proxy",
		"template": "DataIO.Version6.GetTypeNames",
		"connectionsReference": "authentication",
		"params": {
		  "uri": "<PROXY_BASE_URL>/api/dataio/getTypeNames"
		}
	  },
	  {
		"name": "GetTypeDefinitions Action",
		"description": "something about data io to a proxy",
		"template": "DataIO.Version6.GetTypeDefinitions",
		"connectionsReference": "authentication",
		"params": {
		  "uri": "<PROXY_BASE_URL>/api/dataio/getTypeDefinitions"
		}
	  }
	],
	"publicationRegions": [
		"US"
	],
	"distribution": "PRIVATE"
  }